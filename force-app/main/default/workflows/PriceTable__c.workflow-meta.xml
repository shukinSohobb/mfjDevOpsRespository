<?xml version="1.0" encoding="UTF-8"?>
<Workflow xmlns="http://soap.sforce.com/2006/04/metadata">
    <alerts>
        <fullName>PriceTableFinalApprovalAlert</fullName>
        <description>内部：価格管理の申請承認</description>
        <protected>false</protected>
        <recipients>
            <type>owner</type>
        </recipients>
        <senderType>CurrentUser</senderType>
        <template>MFJEmailTemplate/PriceTableApprovalRequestApproved</template>
    </alerts>
    <alerts>
        <fullName>PriceTableFinalRejectionAlert</fullName>
        <description>内部：価格管理の申請却下</description>
        <protected>false</protected>
        <recipients>
            <type>owner</type>
        </recipients>
        <senderType>CurrentUser</senderType>
        <template>MFJEmailTemplate/PriceTableApprovalRequestRejected</template>
    </alerts>
    <fieldUpdates>
        <fullName>UpdatePriceTableActivate</fullName>
        <field>Active__c</field>
        <literalValue>1</literalValue>
        <name>価格管理の有効（手動）：True</name>
        <notifyAssignee>false</notifyAssignee>
        <operation>Literal</operation>
        <protected>false</protected>
        <reevaluateOnChange>true</reevaluateOnChange>
    </fieldUpdates>
    <fieldUpdates>
        <fullName>UpdatePriceTableStatusApproved</fullName>
        <description>価格管理オブジェクトの状況の項目を承認に更新します。</description>
        <field>Status__c</field>
        <literalValue>Approved</literalValue>
        <name>価格管理の状況：承認</name>
        <notifyAssignee>false</notifyAssignee>
        <operation>Literal</operation>
        <protected>false</protected>
        <reevaluateOnChange>true</reevaluateOnChange>
    </fieldUpdates>
    <fieldUpdates>
        <fullName>UpdatePriceTableStatusPendingApproval</fullName>
        <description>価格管理オブジェクトの状況の項目を承認待ちに更新します。</description>
        <field>Status__c</field>
        <literalValue>PendingApproval</literalValue>
        <name>価格管理の状況：承認待ち</name>
        <notifyAssignee>false</notifyAssignee>
        <operation>Literal</operation>
        <protected>false</protected>
        <reevaluateOnChange>true</reevaluateOnChange>
    </fieldUpdates>
    <fieldUpdates>
        <fullName>UpdatePriceTableStatusRejected</fullName>
        <description>価格管理オブジェクトの状況の項目を却下に更新します。</description>
        <field>Status__c</field>
        <literalValue>Rejected</literalValue>
        <name>価格管理の状況：却下</name>
        <notifyAssignee>false</notifyAssignee>
        <operation>Literal</operation>
        <protected>false</protected>
        <reevaluateOnChange>true</reevaluateOnChange>
    </fieldUpdates>
    <fieldUpdates>
        <fullName>UpdatePriceTableStatusRevising</fullName>
        <description>価格管理オブジェクトの状況の項目を修正中に更新します。</description>
        <field>Status__c</field>
        <literalValue>Revising</literalValue>
        <name>価格管理の状況：修正中</name>
        <notifyAssignee>false</notifyAssignee>
        <operation>Literal</operation>
        <protected>false</protected>
        <reevaluateOnChange>true</reevaluateOnChange>
    </fieldUpdates>
</Workflow>
