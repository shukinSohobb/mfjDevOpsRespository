        1
    ),
    VALUE(MID({!$Record.WorkStartDate__c}, FIND(&quot;月&quot;, {!$Record.WorkStartDate__c}) + 1, FIND(&quot;日&quot;, {!$Record.WorkStartDate__c}) - FIND(&quot;月&quot;, {!$Record.WorkStartDate__c}) - 1))
)</expression>
