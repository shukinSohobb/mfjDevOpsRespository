    <formulas>
        <name>ClockInTimeData</name>
        <dataType>DateTime</dataType>
        <expression>DATETIMEVALUE(
    TEXT(TODAY()) &amp; &quot; &quot; &amp; 
    LEFT({!$Record.ClockInTime__c}, FIND(&quot;:&quot;, {!$Record.ClockInTime__c}) - 1) &amp; &quot;:&quot; &amp; 
    MID({!$Record.ClockInTime__c}, FIND(&quot;:&quot;, {!$Record.ClockInTime__c}) + 1, 2) &amp; &quot;:00&quot;
)</expression>
    </formulas>
    <formulas>
