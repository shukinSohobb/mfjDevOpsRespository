    <formulas>
        <description>0 - 登録画面の「在庫調整数/倉庫移動数」</description>
        <name>MinusProductStockAdjustQuantity</name>
        <dataType>Number</dataType>
        <expression>0 - {!ProductStockAdjustQuantity}</expression>
        <scale>0</scale>
    </formulas>
    <formulas>
        <name>NewRecordUrl</name>
        <dataType>String</dataType>
        <expression>LEFT({!$Api.Partner_Server_URL_260}, FIND( &apos;/services&apos;, {!$Api.Partner_Server_URL_260}))
&amp; &quot;lightning/r/ProductStockInfo__c/&quot; &amp; {!ID_F} &amp; &quot;/view&quot;</expression>
    </formulas>
    <formulas>
        <name>WarehouseMasterCheck</name>
        <dataType>Boolean</dataType>
        <expression>ISBLANK({!WarehouseMaster.recordId})
&amp;&amp;
NOT(ISBLANK({!WarehouseTransferRequest.recordId}))</expression>
    </formulas>
    <formulas>
        <name>WarehouseTransferRequestCheck</name>
        <dataType>Boolean</dataType>
        <expression>ISBLANK({!WarehouseTransferRequest.recordId})
&amp;&amp;
NOT(ISBLANK({!WarehouseMaster.recordId}))</expression>
    </formulas>
