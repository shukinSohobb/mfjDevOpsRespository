    <formulas>
        <name>ArrivalQuantity_F</name>
        <dataType>Number</dataType>
        <expression>IF (ISBLANK({!GetProductStockInfo.ArrivalQuantity__c}), 0, {!GetProductStockInfo.ArrivalQuantity__c})+ IF ({!ProductStockAdjustQuantity} &lt; 0, {!ProductStockAdjustQuantity}, (0-{!ProductStockAdjustQuantity}))</expression>
