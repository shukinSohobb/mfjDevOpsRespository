<?xml version="1.0" encoding="UTF-8"?>
<Flow xmlns="http://soap.sforce.com/2006/04/metadata">
    <apiVersion>63.0</apiVersion>
    <areMetricsLoggedToDataCloud>false</areMetricsLoggedToDataCloud>
    <customErrors>
        <name>errorMessage</name>
        <label>運賃手動計算フラグエラー</label>
        <locationX>176</locationX>
        <locationY>323</locationY>
        <customErrorMessages>
            <errorMessage>「運賃#」自動的に設定されませんでした。「料金」を手動設定してください。</errorMessage>
            <isFieldError>false</isFieldError>
        </customErrorMessages>
    </customErrors>
    <environments>Default</environments>
    <interviewLabel>注文の {!$Flow.CurrentDateTime}</interviewLabel>
    <label>注文の運賃手動計算フラグチェック</label>
    <processMetadataValues>
        <name>BuilderType</name>
        <value>
            <stringValue>LightningFlowBuilder</stringValue>
        </value>
    </processMetadataValues>
    <processMetadataValues>
        <name>CanvasMode</name>
        <value>
            <stringValue>AUTO_LAYOUT_CANVAS</stringValue>
        </value>
    </processMetadataValues>
    <processMetadataValues>
        <name>OriginBuilderType</name>
        <value>
            <stringValue>LightningFlowBuilder</stringValue>
        </value>
    </processMetadataValues>
    <processType>AutoLaunchedFlow</processType>
    <start>
        <locationX>50</locationX>
        <locationY>0</locationY>
        <connector>
            <targetReference>errorMessage</targetReference>
        </connector>
        <filterFormula>NOT (
    ISNEW()
    || 
    ISPICKVAL({!$Record.Status__c}, &apos;Reception&apos;)
)
&amp;&amp;
NOT {!$Record.ShippingFeeManualFlag__c}
&amp;&amp;
ISBLANK({!$Record.ShippingFee__c})</filterFormula>
        <object>OrderHeader__c</object>
        <recordTriggerType>CreateAndUpdate</recordTriggerType>
        <triggerType>RecordAfterSave</triggerType>
    </start>
    <status>Active</status>
    <triggerOrder>1500</triggerOrder>
</Flow>
