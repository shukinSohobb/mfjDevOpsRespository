    <formulas>
        <name>ResultTypeArg</name>
        <dataType>String</dataType>
        <expression>CASE({!Index}, 
0, &quot;TermCycleStart&quot;, 
1, &quot;TermCycleEnd&quot;,
2, &quot;TermStart&quot;,
3, &quot;TermEnd&quot;,
&quot;&quot;)</expression>
