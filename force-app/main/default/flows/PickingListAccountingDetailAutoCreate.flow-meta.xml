    <formulas>
        <description>TRUE：削除。FALSE:削除しない</description>
        <name>ToBeDeletedFlag</name>
        <dataType>Boolean</dataType>
        <expression>IF(
   ISPICKVAL({!$Record.ProductStockInfo__r.Account__r.ClosingDate__c}, &quot;MONTH_15&quot;),
    IF(
        DAY({!ShippedDetailRecordDate}) &lt;&gt; 6
        &amp;&amp; DAY({!ShippedDetailRecordDate}) &lt;&gt; 16
        &amp;&amp; DAY({!ShippedDetailRecordDate}) &lt;&gt; 26,
        TRUE,
        FALSE
    ),
    IF(
        DAY({!ShippedDetailRecordDate}) &lt;&gt; 1
        &amp;&amp; DAY({!ShippedDetailRecordDate}) &lt;&gt; 11
        &amp;&amp; DAY({!ShippedDetailRecordDate}) &lt;&gt; 21,
        TRUE,
        FALSE
    )
)</expression>
    </formulas>
